{
  "baseConfig": "trifid:config-sparql.json",
  "sparqlEndpointUrl": "env:SPARQL_ENDPOINT_URL",
  "sparqlEndpointAuthentication": {
    "user": "env:SPARQL_USERNAME",
    "password": "env:SPARQL_PASSWORD"
  },
  "sparqlProxy": {
    "default": {
      "options": {
        "queryOperation": "postQueryUrlencoded"
      }
    }
  },
  "staticViews": {
    "/": "cwd:views/home.html",
    "/linked-data-day/": "cwd:views/linked-data-day.html",
    "/datasets/": "cwd:views/datasets.html",
    "/datasets-int/": "cwd:views/datasets-int.html",
    "/sparql/": "cwd:views/yasgui.html",
    "/contact/": "cwd:views/contact.html",
    "/documentation/": "cwd:views/page.html",
    "/documentation/*": "cwd:views/page.html",
    "/governance/": "cwd:views/page.html",
    "/governance/*": "cwd:views/page.html",
    "/technology/": "cwd:views/page.html",
    "/technology/*": "cwd:views/page.html"

  },
  "staticFiles": {
    "dataadminFiles": {
      "path": "/",
      "folder": "cwd:public"
    },
    "styleFiles": {
      "path": "/",
      "folder": "cwd:node_modules/swiss-styleguide/build"
    },
    "zackFiles": {
      "path": "/datasets/",
      "folder":  "cwd:node_modules/zack-search/dist/"
    }
  },
  "handler": {
    "root": {
      "options": {
        "resourceExistsQuery": "ASK { GRAPH ?g { <${iri}> ?p ?o }}",
        "resourceGraphQuery": "#pragma describe.strategy cbd\nDESCRIBE <${iri}>",
        "containerExistsQuery": "ASK { GRAPH ?g { <${iri}> ?p ?o }}",
        "containerGraphQuery": "#pragma describe.strategy cbd\nDESCRIBE <${iri}>"
      }
    }
  },
  "breakDown": {
    "graphexplorer": {},
    "graphexplorer.default": {},
    "graphexplorer.default.endpointUrl": [
      "sparqlProxy.default.path",
      "sparqlEndpointUrl"
    ],
    "spex": {},
    "spex.default": {},
    "spex.default.options": {},
    "spex.default.options.url": [
      "sparqlProxy.default.path",
      "sparqlEndpointUrl"
    ],
    "ckan": {},
    "ckan.default": {},
    "ckan.default.endpointUrl": [
      "sparqlProxy.default.path",
      "sparqlEndpointUrl"
    ]
  },
  "spex": {
    "default": {
      "path": "/spex",
      "options": {
        "prefixes": []
      }
    }
  },
  "ckan": {
    "default": {
      "path": "/ckan"
    }
  },
  "graphexplorer": {
    "default": {
      "template": "cwd:views/graph-explorer.html",
      "path": "/graph-explorer",
      "acceptBlankNodes": false,
      "dataLabelProperty": "rdfs:label | <http://schema.org/name>",
      "schemaLabelProperty": "rdfs:label | <http://schema.org/name>",
      "language": "en",
      "languages": [
        {
          "code": "en",
          "label": "English"
        },
        {
          "code": "de",
          "label": "German"
        },
        {
          "code": "fr",
          "label": "French"
        },
        {
          "code": "it",
          "label": "Italian"
        }
      ]
    }
  },
  "i18n": {
    "updateFiles": false,
    "locales": ["de", "fr", "it", "en"]
  },
  "renderer": {
    "root": {
      "template": "cwd:views/index.html",
      "templateError": "cwd:views/error.html"
    }
  },
  "yasgui": {
    "default": {
      "template": "cwd:views/yasgui.html",
      "urlShortener": "https://s.zazuko.com/api/v1/shorten"
    }
  },
  "content": {
    "navigation": [
      {
        "title": "Linked Data Day",
        "link": "/linked-data-day/"
      },
      {
        "title": "About",
        "link": "/"
      },
      {
        "title": "Datasets",
        "link": "/datasets/"
      },
      {
        "title": "Query Data",
        "link": "/sparql/"
      },
      {
        "title": "Documentation",
        "link": "/documentation/",
        "subpages": [
          {
            "title": "Administrative",
            "link": "/documentation/admin/"
          },
          {
            "title": "Technical",
            "link": "/documentation/technical/"
          },
          {
            "title": "Applications",
            "link": "/documentation/applications/"
          }
        ]
      },
      {
        "title": "Linked Data Governance",
        "link": "/governance/",
        "subpages": [
          {
            "title": "Namespace",
            "link": "/governance/namespace/"
          },
          {
            "title": "Architecture",
            "link": "/governance/architecture/"
          },
          {
            "title": "Core Concepts",
            "link": "/governance/core-concepts/"
          },
          {
            "title": "Guidelines",
            "link": "/governance/guidelines/"
          }
        ]
      },
      {
        "title": "Linked Data Technology",
        "link": "/technology/",
        "subpages": [
          {
            "title": "General",
            "link": "/technology/general/"
          },
          {
            "title": "Tools",
            "link": "/technology/tools/"
          },
          {
            "title": "Glossary",
            "link": "/technology/glossary/"
          },
          {
            "title": "Help",
            "link": "/technology/help/"
          }
        ]
      }
    ]
  },
  "rdfjsProxy": {
    "root": {}
  },
  "plugins": {
    "staticFiles": {
      "priority": 130
    },
    "i18n": {
      "priority": 5,
      "module": "trifid-plugin-i18n"
    },
    "readFile": {
      "priority": 110,
      "module": "./trifid/readFile.js"
    },
    "graphexplorer": {
      "priority": 114,
      "module": "trifid-plugin-graph-explorer"
    },
    "spex": {
      "priority": 114,
      "module": "trifid-plugin-spex"
    },
    "ckan": {
      "priority": 114,
      "module": "@zazuko/trifid-plugin-ckan"
    },
    "rdfjsProxy": {
      "module": "./trifid/rdfjsProxy.js"
    }
  }
}
